sum_odd=0
sum_even=0
total=0

card_number =input("enter credit card #: ")
card_number = card_number.replace("-","")
card_number = card_number.replace(" ","")
card_number = card_number[::-1]

for x in card_number[::2]:
    sum_odd += int(x)
for x in card_number[1::2]:
    x= int(x)*2
    if x>=10:
        sum_even+= (1+(x%10))
    else:
        sum_even += x
    
total = sum_odd+sum_even

if total %10 ==0:
    print("VALID")
else:
    print("INVALID")
